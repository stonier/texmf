@ARTICLE{bala_95:modelling_slip,
  author = {Balakrishna, R. and Ghosal, A.},
  title = {Modelling of Slip for Wheeled Mobile Robots},
  journal = {IEEE Transactions on Robotics and Automation},
  year = {1995},
  volume = {11},
  pages = {126-132},
  number = {1},
  abstract = {Wheeled Mobile Robots (WMRs) are known to be non-
	
	holonomic systems, and most dynamic models of WMRs assume that
	
	the wheels undergo rolling without slipping. This paper deals with
	the
	
	problem of modeling and simulation of motion of a WMR when the
	
	conditions for rolling are not satisfied at the wheels. We use a traction
	
	model where the adhesion coefficient between the wheels of a WMR and
	
	a hard flat surface is a function of the wheel slip. This traction
	model is
	
	used in conjunction with the dynamic equations of motion to simulate
	the
	
	motion of the WMR. The simulations show that controllers which do
	not
	
	take into account wheel slip give poor tracking performance for the
	WMR
	
	and path deviation is small only for large adhesion coefficients.
	This work
	
	shows the importance of wheel slip and suggests use of accurate traction
	
	models for improving tracking performance of a WMR.},
  keywords = {slip control},
  pdf = {papers/omnidirectional_motion/slip/95_modelling_slip_balakrishna.pdf},
  review = {Generates the nonlinear slip equation for a 3 wheeled omnidirectional
	robot (just like ours). Then proceeds to do model reference control
	on the system. The ideal rolling condition is not used and a Lyapunov
	proof is given for the tracking control scheme.},
  url = {http://localhost/work/papers/omnidirectional_motion/slip/95_modelling_slip_balakrishna.pdf}
}

