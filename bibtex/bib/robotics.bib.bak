@CONFERENCE{li_05:slip_minimisation,
  author = {Liu, Y. and Oetomo, D. and Macelo, H.},
  title = {Torque Distribution and Slip Minimization in an Omnidirectional Mobile
	Base},
  booktitle = {Proceedings of International Conference on Advanced Robotics},
  year = {2005},
  pages = {567-572},
  abstract = {Two forward kinematic models which are used in the control of an omnidirectional
	mobile base are evaluated. These two models result in different sensitivities
	to joint position error. The analysis and experimental results in
	this paper demonstrate the capabilities of dynamic model in improving
	the sensitivity of the forward kinematic model, resulting in a more
	even distribution of joint torques and in minimizing the amount of
	slip between wheels.},
  keywords = {slip control},
  pdf = {papers/omnidirectional_motion/slip/05-slip_minimisation_liu.pdf},
  review = {Uses powered caster wheels instead of omni-wheels. But there's redundancy
	in these. They compare the results of a torque minimisation scheme
	with basic kinematic control with some reference to slippage. Details
	are lacking on the kinematic control side of things, but its a good
	starting point for checking out various systesms. They provide references
	leading to work on operational space formulation and an orthogonal
	complement derivation for the equations of motion.},
  url = {papers/omnidirectional_motion/slip/05-slip_minimisation_liu.pdf}
}

